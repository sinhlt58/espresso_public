[{
    "scope": "GLOBAL",
    "patterns": [
        "DenyPathQuery \\.jpg"
    ]
},
{
    "scope": "domain:batdongsan.com.vn",
    "patterns": [
        "AllowPathQuery \/ban-|\/cho-thue-|\/mua-|\/can-thue-",
        "DenyPathQuery .+"
    ]
},
{
    "scope": "domain:lazada.vn",
    "patterns": [
        "AllowPathQuery products(.*)search=1",
        "AllowPathQuery page=",
        "DenyPathQuery .+"
    ]
},
{
    "scope": "domain:shopee.vn",
    "patterns": [
        "AllowPathQuery ([0-9]{2,})\\.([0-9]{2,})",
        "AllowPathQuery page=",
        "DenyPathQuery .+"
    ]
},
{
    "scope": "metadata:key=value",
    "patterns": [
       "DenyPath .+"
    ]
}
]